{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "maxson-portfolio-projects/portfolio",
  "description": "Display all portfolio projects",
  "version": "1.0.0",
  "title": "Portfolio",
  "keywords": [
    "grid",
    "projects",
    "previews"
  ],
  "category": "portfolioprojects",
  "icon": "portfolio",
  "attributes": {
    "showFilters": {
      "type": "boolean",
      "default": true
    },
    "columns": {
      "type": "number",
      "default": 3
    },
    "requireThumb": {
      "type": "boolean",
      "default": true
    },
    "numberOfItems": {
      "type": "number",
      "default": 100
    },
    "order": {
      "type": "string",
      "default": "DESC"
    },
    "orderBy": {
      "type": "string",
      "default": "date"
    }
  },
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": false,
      "text": false
    },
    "typography": {
      "fontSize": false
    }
  },
  "textdomain": "maxson",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}