{
	"name": "archive",
	"version": "1.0.0",
	"description": "Portfolio Projects by Maxson.",
	"author": "Thomas Maxson",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=src/ --output-path=build/ --webpack-copy-php",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=src/ --output-path=build/ --webpack-copy-php",
		"watch": "watch 'npm run build' ./src",
		"start:portfolio": "wp-scripts start --webpack-src-dir=src/portfolio --output-path=build/portfolio",
		"build:portfolio": "wp-scripts build --webpack-src-dir=src/portfolio --output-path=build/portfolio",
		"start:filter": "wp-scripts start --webpack-src-dir=src/filter --output-path=build/filter",
		"build:filter": "wp-scripts build --webpack-src-dir=src/filter --output-path=build/filter",
		"start:carousel": "wp-scripts start --webpack-src-dir=src/carousel --output-path=build/carousel",
		"build:carousel": "wp-scripts build --webpack-src-dir=src/carousel --output-path=build/carousel"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.19.0"
	},
	"dependencies": {
		"@wordpress/icons": "^9.40.0",
		"classnames": "^2.5.1",
		"lodash": "^4.17.21",
		"watch": "^1.0.2"
	}
}
